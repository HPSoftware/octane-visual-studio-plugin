<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.VisualStudio.CoreUtility" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-15.0.0.0" newVersion="15.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <appSettings>
    <add key="webAppUrl" value="octane-url"/>

    <add key="userName" value="userName"/>
    <add key="password" value="password"/>

    <add key="sharedSpaceId" value="1001"/>
    <add key="workspaceId" value="1002"/>

    <!-- If you use secured connection(https) but sertificates are not installed correctly, set value to true to ignore validation -->
    <add key="ignoreServerCertificateValidation" value="false"/>
  </appSettings>
</configuration>